# API runtime
TRYON_ENGINE=stableviton
TRYON_MODEL_VERSION=v1

# Paths inside the container (mounted from host where noted)
TRYON_OUTPUTS_DIR=/tmp/tryon/outputs
TRYON_UPLOADS_DIR=/tmp/tryon/uploads
TRYON_MODELS_DIR=/models

# Models (set one of URL or mount files under ./models)
STABLEVITON_SHAREPOINT_URL=
SCHP_DRIVE_URL=https://drive.google.com/uc?id=1k4dllHpu0bdx38J7H28rVVLpU-kOHmnH
INSTANTID_ANTELOPE_URL=https://sourceforge.net/projects/insightface.mirror/files/v0.7/antelopev2.zip/download

# Hugging Face auth for gated files
HUGGINGFACE_HUB_TOKEN=

# GPU options
TRYON_USE_FP16=1
TRYON_MAX_RES=768
NVIDIA_VISIBLE_DEVICES=all
